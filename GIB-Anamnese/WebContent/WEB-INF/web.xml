<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>/html/AnamnesebogenStartseite.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>htwg.in.gib.anam.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AnamnesebogenAServlet</servlet-name>
		<servlet-class>htwg.in.gib.anam.AnamnesebogenAServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AnamnesebogenAServlet</servlet-name>
		<url-pattern>/anamnesebogenAnaesthesie</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>AnamnesebogenOServlet</servlet-name>
		<servlet-class>htwg.in.gib.anam.AnamnesebogenOServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AnamnesebogenOServlet</servlet-name>
		<url-pattern>/anamnesebogenOrthopaedie</url-pattern>
	</servlet-mapping>

</web-app>